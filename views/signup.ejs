<link rel="stylesheet" href="../assets/CS/signin.css">
<div id="signin-body">
  <form action="/adduser" method="post">
  
      <div class="container">
        <label for="name"><b>Name</b></label>
        <input type="text" placeholder="Enter Name" name="name" required>
   
        <label for="phoneNum"><b>Phonenumber</b></label>
       <input type="text" placeholder="Enter PhoneNumber" name="phoneNum" required>
  
       <label for="password"><b>Password</b></label>
       <input type="password" placeholder="Enter Password" name="password" required>
        
       <label for="confirm_password"><b>Password</b></label>
       <input type="password" placeholder="Enter Password" name="confirm_password" required>
  
       <button type="submit">Signup</button>
     </div>
   
     
   </form>
</div>